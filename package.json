{
  "name": "kashyyyk",
  "description": "Minimal and lightweight internationalization plugin for Vuejs",
  "version": "0.9.1",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Matteo Gabriele",
    "email": "m.gabriele.dev@gmail.com"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "fix": "biome check --write",
    "lint": "biome lint",
    "build": "tsdown",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:once": "vitest run",
    "validate": "npm run lint && npm run test:once && npm run build",
    "prepublishOnly": "npm run validate"
  },
  "keywords": [
    "vue",
    "vuejs",
    "translations",
    "i18n",
    "internationalization",
    "minimal",
    "light"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/kashyyyk.d.ts",
      "import": "./dist/kashyyyk.js"
    }
  },
  "main": "./dist/kashyyyk.js",
  "module": "./dist/kashyyyk.js",
  "types": "./dist/kashyyyk.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.5",
    "jsdom": "^26.0.0",
    "semantic-release": "^24.2.3",
    "tsdown": "^0.9.3",
    "vitest": "^3.0.5",
    "vue": "^3.5.13"
  }
}
